@page
@model WebApp.Pages.SavedGames

@{
    ViewData["Title"] = "Load game";
}

<div class="container text-center">
    <h1>Load Game</h1>

    @foreach (var save in Model.BattleShipsSaves)
    {
        <a asp-page="/PlayGame" asp-route-id="@save.Id"><p>@Model.CreateDisplayName(save)</p></a>
    }
</div>

